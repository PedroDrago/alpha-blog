<div>
  <% flash.each do |key, value| %>

    <div class="container col-5">
      <div class="<%= flash_class(key) %> alert alert-dismissible fade show in">
		  <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        <%= value %>
      </div>
    </div>

  
  
  <% end %>
</div>